
<%@taglib prefix="sf" uri="http://www.springframework.org/tags/form"%>

<div class="row">

	<div class=col-sm-1></div>
	<div class="col-sm-10">
		<br>
		<div class="card">

			<div class="card-header">
				<center>
					<h5>New Booking</h5>
				</center>
			</div>
			<br>
			<div class="card-body">
				<div class="row">
					<div class="col-sm-3">
						<input type="text" placeholder="Officer Name" class="form-control">
					</div>
					<div class="col-sm-3">
						<input type="text" placeholder="Mobile Number"
							class="form-control">
					</div>
					<div class="col-sm-3">
						<input type="text" placeholder="Enter Your email"
							class="form-control">
					</div>
					<div class="col-sm-3">
						<input type="text" placeholder="RUID/PFNo/PPO No"
							class="form-control">
					</div>
				</div>
				<br>
				<div class="row">
					<div class="col-sm-3">
						<input type="text" placeholder="Division" class="form-control">
					</div>
					<div class="col-sm-3">
						<input type="text" placeholder="Place" class="form-control">
					</div>
					<div class="col-sm-3">
						<input type="text" placeholder="Adhar No." class="form-control">
					</div>
					<div class="col-sm-3">
						<div class="row">
							<div class="col-sm-6">
								<input type="text" placeholder="From Date" class="form-control">
							</div>
							<div class="col-sm-6">
								<input type="text" placeholder="To Date" class="form-control">
							</div>
						</div>
					</div>
				</div>
				<br>
				<div class="row">
					<div class="col-sm-3">
						<div class="row">
							<div class="col-sm-6">
								<input type="text" placeholder="No of Room" class="form-control">
							</div>
							<div class="col-sm-6">
								<input type="text" placeholder="purpose" class="form-control">
							</div>
						</div>
					</div>
					<div class="col-sm-3">
						<input type="text" placeholder="Enter your Railway"
							class="form-control">
					</div>
					<div class="col-sm-3">
						<input type="text" placeholder="Designation" class="form-control">
					</div>
					<div class="col-sm-3">
						<div class="row">
							<div class="col-sm-6">
								<input type="radio">:Service
							</div>
							<div class="col-sm-6">
								<input type="radio">:Retired
							</div>
						</div>
					</div>
				</div>
				<br>
				<div class="row">
					<div class="col-sm-3">
						<input type="text" placeholder="Guest" class="form-control">
					</div>
					<div class="col-sm-3">
						<input type="text" placeholder="Guest Name" class="form-control">
					</div>
					<div class="col-sm-3">
						<input type="text" placeholder="Guest Mob No."
							class="form-control">
					</div>
					<div class="col-sm-3">
						<input type="text" placeholder="Purpose of visit"
							class="form-control">
					</div>
				</div>
				<br>
				<div class="row">
					<div class="col-sm-5"></div>
					<div class="col-sm-2">
						<input type="button" name="submit" value="submit" />
					</div>
					<div class="col-sm-5"></div>
				</div>
			</div>
		</div>
		<div class=col-sm-1></div>
	</div>
</div>

